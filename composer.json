    "description": "PayU gateway for Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "payu",
        "purchase"
    ],
    "homepage": "https://github.com/bileto/omnipay-payu",
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Machala",
            "email": "jan.machala@bileto.com"
        }
    ],
    "minimum-stability": "stable",
